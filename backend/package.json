{
  "dependencies": {
    "@google-cloud/aiplatform": "^3.31.0",
    "@google-cloud/vertexai": "^1.8.1",
    "@socket.io/redis-adapter": "^8.1.0",
    "@typegoose/typegoose": "^9.8.1",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-jwt": "^7.7.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^8.5.1",
    "migrate-mongo": "^9.0.0",
    "mongo-dot-notation": "^3.1.0",
    "mongoose": "^6.3.2",
    "mongoose-autopopulate": "^0.17.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.8.8",
    "socket.io": "^4.7.5",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn ./src/server.ts",
    "style:linter": "eslint . --fix --ext .ts",
    "style:formatter": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "style": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "migrate:create": "sh -c 'ts-node node_modules/.bin/migrate-mongo create $0 -f migrate-mongo-config.ts' ",
    "migrate:status": "ts-node node_modules/.bin/migrate-mongo status -f migrate-mongo-config.ts",
    "migrate:undo-last": "ts-node node_modules/.bin/migrate-mongo down -f migrate-mongo-config.ts && npm run migrate:status",
    "migrate": "ts-node node_modules/.bin/migrate-mongo up -f migrate-mongo-config.ts"
  },
  "name": "backend",
  "version": "1.0.0",
  "main": "server.js",
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.19.50",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^9.1.0",
    "migrate-mongo": "^9.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
